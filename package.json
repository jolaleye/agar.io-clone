{
  "private": true,
  "name": "agar.io-clone",
  "version": "1.0.0",
  "description": "A clone of agar.io using Socket.IO and HTML5 canvas",
  "scripts": {
    "start": "node server/index.js",
    "start:client": "webpack-dev-server",
    "start:server": "nodemon server/index.js",
    "start:dev": "concurrently -k -n \"client,server\" -c \"cyan,magenta\" \"npm run start:client\" \"npm run start:server\"",
    "build": "webpack"
  },
  "author": "Jonathan Olaleye",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "lodash": "^4.17.5",
    "semantic-ui-css": "^2.3.1",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "sass-loader": "^7.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
